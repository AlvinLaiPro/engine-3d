{
  "name": "engine-3d.js",
  "version": "1.0.0",
  "description": "Simple 3D Game Engine",
  "main": "dist/engine.js",
  "scripts": {
    "build:dev": "rollup -c ./script/rollup.config.js",
    "build:min": "uglifyjs ./dist/engine.dev.js --mangle --source-map ./dist/engine.min.js.map -o ./dist/engine.min.js",
    "release": "npm run build:dev && npm run build:min",
    "start": "electron",
    "server": "preview -p 8002 -e examples",
    "dev": "rollup -w -c ./script/rollup.config.js",
    "test": "npm run build:dev && tap test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cocos-creator/engine-3d.git"
  },
  "keywords": [
    "engine",
    "3d",
    "game"
  ],
  "author": "jwu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cocos-creator/engine-3d/issues"
  },
  "homepage": "https://github.com/cocos-creator/engine-3d/issues",
  "dependencies": {
  },
  "devDependencies": {
    "electron": "^1.6.3",
    "fs-jetpack": "^0.13.0",
    "lstats.js": "^1.2.0",
    "preview-server": "^1.0.7",
    "rollup": "^0.41.4",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
